{
  "name": "git-history",
  "displayName": "Git History fast",
  "description": "View and search complete git history of a repository with ease.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-history.showHistory",
        "title": "Show Git History"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vishnu-rao-k/git-history"
  },
  "license": "GPL-3.0-only",
  "keywords": [
    "git",
    "history",
    "log",
    "vscode"
  ],
  "publisher": "VishnuPrasadRaoK",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "simple-git": "^3.28.0"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
