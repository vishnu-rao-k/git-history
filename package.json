{
  "name": "git-history",
  "displayName": "Git History fast",
  "description": "View and search complete git history of a repository with ease.",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-history.showHistory",
        "title": "Show Git History"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vishnu-rao-k/git-history"
  },
  "license": "GPL-3.0-only",
  "keywords": [
    "git",
    "history",
    "log",
    "vscode"
  ],
  "publisher": "VishnuPrasadRaoK",
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "pretest": "npm run compile-tests",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "simple-git": "^3.28.0"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}